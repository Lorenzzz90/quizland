package question

import (
    "github.com/Lorenzzz90/quizland/view/layout"
    "fmt"
)
    

templ Result(score int){
    @layout.Base(){
        <div>
            <p>Hai totalizzato { fmt.Sprint(score) } punti su 20 domande totali! </p><br/>
            <form action="/" method="get">
                <input class="bg-white p-1.5 rounded-md shadow-lg hover:bg-teal-600 hover:text-white" type="submit" value="Riprova"></input>
            </form>
        </div>
    }
}
